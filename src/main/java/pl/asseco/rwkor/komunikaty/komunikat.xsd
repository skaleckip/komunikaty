<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://zus.pl/schematy/b2bior/" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:kom="http://zus.pl/schematy/b2bior/" version="1.2">
	<xs:element name="Komunikat" type="kom:KomunikatTyp"/>
	<xs:simpleType name="BiegWnioskuTyp">
		<xs:annotation>
			<xs:documentation>Bieg wniosku</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DataTyp">
		<xs:annotation>
			<xs:documentation>Data</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:date"/>
	</xs:simpleType>
	<xs:simpleType name="DecyzjaTyp">
		<xs:annotation>
			<xs:documentation>Decyzja o ustaleniu bądź odmowie ustalenia zbiegu świadczenia z rentą rodzinną</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Odmowa"/>
			<xs:enumeration value="Ustalenie"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IdentyfikatorBankuTyp">
		<xs:annotation>
			<xs:documentation>Międzynarodowy identyfikator banku w formacie BIC/SWIFT</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="11"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ImieTyp">
		<xs:annotation>
			<xs:documentation>Imię</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="22"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="JednostkaOrganuRentowegoTyp">
		<xs:annotation>
			<xs:documentation>Jednostka organu rentowego</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="4"/>
			<xs:maxLength value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="KodBleduTyp">
		<xs:annotation>
			<xs:documentation>Kod błędu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="KodIdentBankuMacierzystegoTyp">
		<xs:annotation>
			<xs:documentation>Kod identyfikacyjny banku macierzystego w USA</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="35"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="KodPocztowyTyp">
		<xs:annotation>
			<xs:documentation>Kod pocztowy</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="KodRodzajuDodatkuTyp">
		<xs:annotation>
			<xs:documentation>Kod rodzaju dodatku</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="7"/>
			<xs:maxLength value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="KwotaTyp">
		<xs:annotation>
			<xs:documentation>Typ danych odpowiadający liczbie reprezentującej kwotę</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:pattern value="[\d]+\.[\d]{2}"/>
			<xs:fractionDigits value="2"/>
			<xs:totalDigits value="17"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LiczbaTyp">
		<xs:annotation>
			<xs:documentation>Liczba</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0123456789]*"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MiedzynarodowyNrRachunkuTyp">
		<xs:annotation>
			<xs:documentation>Międzynarodowy numer rachunku</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="34"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MiejsceWystapieniaTyp">
		<xs:annotation>
			<xs:documentation>Miejsce wystąpienia</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="2000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MiejscowoscTyp">
		<xs:annotation>
			<xs:documentation>Miejscowość</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="56"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NazwaAdresBankuTyp">
		<xs:annotation>
			<xs:documentation>Nazwa i adres banku</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="500"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NazwaPanstwaTyp">
		<xs:annotation>
			<xs:documentation>Nazwa państwa</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="3"/>
			<xs:maxLength value="42"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NazwiskoTyp">
		<xs:annotation>
			<xs:documentation>Nazwisko</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="31"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NrDowoduTozsamosciTyp">
		<xs:annotation>
			<xs:documentation>Numer dowodu tożsamości</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="15"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NrRachunkuUSATyp">
		<xs:annotation>
			<xs:documentation>Numer rachunku USA</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="39"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NumerDomuLokaluTyp">
		<xs:annotation>
			<xs:documentation>Numer domu / lokalu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="12"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NumerRachunkuTyp">
		<xs:annotation>
			<xs:documentation>Numer rachunku</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="26"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NumerSwiadczeniaTyp">
		<xs:annotation>
			<xs:documentation>Numer świadczenia </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="40"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OrganRentowyTyp">
		<xs:annotation>
			<xs:documentation>Organ rentowy</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PESELTyp">
		<xs:annotation>
			<xs:documentation>Numer PESEL</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="|\d{11}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PowodZadaniaTyp">
		<xs:annotation>
			<xs:documentation>Powód żądania</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PowodZmianySwiadczeniaTyp">
		<xs:annotation>
			<xs:documentation>Powód zmiany świadczenia</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PowodZmianyTyp">
		<xs:annotation>
			<xs:documentation>Powód zmiany</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PowodZmianyZbieguTyp">
		<xs:annotation>
			<xs:documentation>Powód zmiany zbiegu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ProcentTyp">
		<xs:annotation>
			<xs:documentation>Typ danych reprezentujący procent</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:maxInclusive value="100"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ProporcjaSwiadczeniaWZbieguTyp">
		<xs:annotation>
			<xs:documentation>Możliwa proporcja świadczenia - w skład jakiego zbiegu może wchodzić dane świadczenie /składnik</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RodzajAdresuTyp">
		<xs:annotation>
			<xs:documentation>Rodzaj adresu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Adres"/>
			<xs:enumeration value="Poste restante"/>
			<xs:enumeration value="Skrytka pocztowa"/>
			<xs:enumeration value="Przegródka pocztowa"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RodzajDokumentuTyp">
		<xs:annotation>
			<xs:documentation>Rodzaj dokumentu tożsamości</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RodzajRachunkuUsaTyp">
		<xs:annotation>
			<xs:documentation>Rodzaj rachunku</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="B">
				<xs:annotation>
					<xs:documentation>bieżący</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="O">
				<xs:annotation>
					<xs:documentation>oszczędnościowy</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RodzajSwiadczeniaDlaZbieguWdowiegoTyp">
		<xs:annotation>
			<xs:documentation>Rodzaj świadczenia dla zbiegu wdowiego</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RodzajSwiadczeniaTyp">
		<xs:annotation>
			<xs:documentation>Rodzaj świadczenia</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="7"/>
			<xs:maxLength value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SposobZmniejszeniaSkladnikaWSwiadczeniuTyp">
		<xs:annotation>
			<xs:documentation>Sposób zmniejszenia składnika w wypłacanym procencie świadczenia w zbiegu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StanPrawaTyp">
		<xs:annotation>
			<xs:documentation>Stan prawa do świadczenia</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StanSwiadczeniaTyp">
		<xs:annotation>
			<xs:documentation>Stan świadczenia</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="2"/>
			<xs:maxLength value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StopienPokrewienstwaTyp">
		<xs:annotation>
			<xs:documentation>Stopien pokrewieństwa</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[\d]*"/>
			<xs:minLength value="1"/>
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StronaWyplacajacaSwiadczenieTyp">
		<xs:annotation>
			<xs:documentation>Strona umowy międzynarodowej wypłacająca świadczenie</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="3"/>
			<xs:maxLength value="42"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SygnaturaTyp">
		<xs:annotation>
			<xs:documentation>Sygnatura</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SymbolDodatkuTyp">
		<xs:annotation>
			<xs:documentation>Symbol dodatku</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SymbolGrupyFinansowaniaPomniejszeniaTyp">
		<xs:annotation>
			<xs:documentation>Symbol grupy finansowania / pomniejszenia</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SymbolKomunikatuTyp">
		<xs:annotation>
			<xs:documentation>Symbol komunikatu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="K00">
				<xs:annotation>
					<xs:documentation>Informacja o wniosku o ustalenie zbiegu świadczeń z rentą rodzinną ERWD</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="K01">
				<xs:annotation>
					<xs:documentation>Żądanie potwierdzenia występowania świadczeń i ich wysokości</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="K02">
				<xs:annotation>
					<xs:documentation>Odpowiedź na żądanie potwierdzenia występowania świadczeń i ich wysokości</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="K03">
				<xs:annotation>
					<xs:documentation>Informacja o rozstrzygnięciu: odmowie lub ustaleniu zbiegu świadczenia z rentą rodzinną</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="K04">
				<xs:annotation>
					<xs:documentation>Informacja o ustaniu prawa lub zmianie wysokości świadczeń</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="K05">
				<xs:annotation>
					<xs:documentation>Ponaglenie dotyczące potwierdzenia występowania świadczeń w organie rentowym oraz jego wysokości</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="K06">
				<xs:annotation>
					<xs:documentation>Informacja o zmianie ustalonego zbiegu świadczeń z rentą rodzinną</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="KPB">
				<xs:annotation>
					<xs:documentation>Informacja o potwierdzeniu przyjęcia komunikatu albo błędach uniemożliwiających jego przyjęcie</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SymbolSwiadczeniaTyp">
		<xs:annotation>
			<xs:documentation>Symbol świadczenia</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SymbolWariantuZbieguTyp">
		<xs:annotation>
			<xs:documentation>Symbol wariantu zbiegu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TakNieTyp">
		<xs:annotation>
			<xs:documentation>Tak lub Nie</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Tak"/>
			<xs:enumeration value="Nie"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Tekst200Typ">
		<xs:annotation>
			<xs:documentation>Tekst o długości maksymalnie 200 znaków</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="200"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Tekst20Typ">
		<xs:annotation>
			<xs:documentation>Tekst o długości maksymalnie 20 znaków</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TelefonTyp">
		<xs:annotation>
			<xs:documentation>Numer telefonu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[+]?[0-9 -]{0,14}"/>
			<xs:maxLength value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TerminPlatnosciTyp">
		<xs:annotation>
			<xs:documentation>Termin płatności</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{1,2}"/>
			<xs:minLength value="1"/>
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TrybPotwierdzeniaTyp">
		<xs:annotation>
			<xs:documentation>Tryb potwierdzenia</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TrybUstalaniaZbieguTyp">
		<xs:annotation>
			<xs:documentation>Tryb ustalania zbiegu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="UlicaTyp">
		<xs:annotation>
			<xs:documentation>Ulica</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="100"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="UUIDTyp">
		<xs:annotation>
			<xs:documentation>Identyfikator</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="36"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="WariantZbieguTyp">
		<xs:annotation>
			<xs:documentation>Wariant zbiegu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="WersjaSchematuTyp">
		<xs:annotation>
			<xs:documentation>Wersja schematu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AdresKorespondencyjnyTyp">
		<xs:annotation>
			<xs:documentation>Adres korespondencyjny</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RodzajAdresu" type="kom:RodzajAdresuTyp" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Rodzaj adresu</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Ulica" type="kom:UlicaTyp" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Ulica</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumerDomu" type="kom:NumerDomuLokaluTyp" minOccurs="0" maxOccurs="1">
